{
  "name": "nodejscafe",
  "version": "1.0.0",
  "description": "scrapper for nodejs positions",
  "main": "index.js",
  "scripts": {
    "lint": "eslint 'src/**/*.ts'",
    "lint:fix": "npm run lint -- --fix",
    "test": "jest --passWithNoTests --no-cache --runInBand",
    "test:watch": "npm test -- --watch",
    "test:staged": "npm test -- --findRelatedTests",
    "test:coverage": "npm test -- --coverage",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Sergio-Pedretti/nodejscafe.git"
  },
  "keywords": [
    "nodejs",
    "position",
    "search"
  ],
  "author": "sergiopedretti and marcosmoraes",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Sergio-Pedretti/nodejscafe/issues"
  },
  "homepage": "https://github.com/Sergio-Pedretti/nodejscafe#readme",
  "devDependencies": {
    "@types/jest": "^29.5.3",
    "@types/module-alias": "^2.0.1",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "eslint": "^8.45.0",
    "eslint-config-standard-with-typescript": "^37.0.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-n": "^16.0.1",
    "eslint-plugin-promise": "^6.1.1",
    "git-commit-msg-linter": "^5.0.4",
    "husky": "^8.0.0",
    "jest": "^29.6.1",
    "jest-mock-extended": "^3.0.4",
    "lint-staged": "^13.2.3",
    "ts-jest": "^29.1.1",
    "typescript": "^5.1.6"
  },
  "dependencies": {
    "module-alias": "^2.2.3",
    "puppeteer": "^20.9.0"
  }
}
